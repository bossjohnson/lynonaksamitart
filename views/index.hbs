<main>
    <div class="infoPane">

    </div>
    <div class="changeImg">
        <span id="prev" class="fa fa-3x fa-chevron-left"></span>
    </div>
    <div class="mainCenter">
        <img src="{{ latestUrl }}" alt="" />
        <span class="showInfo fa fa-large fa-info-circle"></span>
    </div>
    <div class="changeImg">
        <span id="next" class="fa fa-3x fa-chevron-right"></span>
    </div>
</main>

<div class="thumbnails">

</div>

<footer>
    <div>
        <img src="images/logo.png" alt="Lynon Aksamit">
