        <div id="categories">
            <a class="category showAll" href="#">All</a>
            {{#each categories}}
                <a class="category" href="#">{{ this }}</a>
            {{/each}}
        </div>
    </div>
    <div>
        <a href="#">About</a>
        <a href="#">Contact</a>
        <a href="#">Press</a>
        <a class="fa fa-instagram fa-lg" href="https://www.instagram.com/lynonaksamit/" target="_blank"></a>
        <a class="fa fa-facebook-official fa-lg" href="https://www.facebook.com/lynon.aksamit?fref=ts" target="_blank"></a>
